	<script type='text/javascript'>
		function $$(id){return document.getElementById(id);}
		//DWREngine.setAsync(false);
		function jstrim(s)
		{
		    if(s.substr(0,1)==" ")
		    {
		        do
		        {
		            s=s.substr(1,s.length-1);
		        }while(s.substr(0,1)==" ")
		    }
		
		    if(s.substring(s.length,s.length-1)==" ")
		    {
		        do
		        {
		            s=s.substring(0,s.length-1);
		        }while(s.substring(s.length,s.length-1)==" ")
		    }
		
		    return s;
		}
	
		function callBack(data)
		{
		    alert(data);  
		    
		          
		}
	
		function sms(flag)
		{
		    var username = jstrim(document.getElementById("username").value);
		    var userReg = /^1[1-9][\d]{9}$/;
		    if(username == "" || username == "移动手机号")
		    {
		        alert("请输入正确的手机号码!");
		        return false;
		    }
		    
		    if(!username.match(userReg))
		    {
		        alert("您输入的号码(" + username + ")不支持申请动态密码!");
		        return false;
		    }

		    
	            
	            
	                PasswordService.smsSendPassword(username, callBack);
	            
	        
		}
		
		function checkLogin()
		{
		    checkLoginData();
		}
	
		function checkUserName()
		{
			var regExp = /^[\w]{3,18}$/;
			var username = document.getElementById("username");
		    if(username == null || username.value == "" 
		       || username.value == "移动手机号")
		    {
		        alert("请输入正确的手机号码!");
		        username.select();
		        return false;
		    }
		    if(!username.value.match(regExp))
		    {
		        alert("请输入正确的手机号码!");
		        username.select();
		        return false;
		    }
		    return true;
		}
	
		function checkPassword()
		{
			var regExp = /^[\w]{6,16}$/;
			var password = document.getElementById("password");

			if(!password.value.match(regExp))
		    {
		        alert("请输入6-16位长度的密码!");
		        password.select();
		        return false;
		    }
		    return true;
		}
	
		function checkLoginData()
		{
		    var ckunResult = checkUserName();
		    if(!ckunResult)
		    {
		    	return false;
		    }
		    
  		    var ckpwd = checkPassword();
		    if(!ckpwd)
		    {
		    	return false;
		    }
		    
		    
	            
	            
				    document.getElementById("loginForm").submit();
				    return true;
	            
	        
		}
	
		function checkVerifyCode()
		{
		    var validate = jstrim($$("VerifyCode").value);
		    if (validate == "" || validate == '输入验证码')
		    {
		        alert("请输入验证码！");
		        return false;
		    }
		    
		    var retn = false;
		    SendSmsCode.validateCode($$("VerifyCode").value, function(data){
		        retn = data;
		    });
		    if(!retn)
		    {
		        alert("验证码错误！");
		        $$("VerifyCode").value = "";
		        $$("VerifyCode").focus();
		        RefreshImage();
		        return false;
		    }
		    return true;
		}
		  
		function RefreshImage()
		{
		    var el = $$("validateimgid");
		    el.src = el.src + '?';
		}
	</script>
